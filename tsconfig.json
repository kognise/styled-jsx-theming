{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "jsx": "react",
    "declaration": true,
    "lib": [ "es6", "dom" ],
    "outDir": "dist/"
  },
  "include": [ "src/" ]
}